@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Demonstration of the output surface (a query filter)

:Alice a :Person .

(_:S) log:onNegativeSurface {
    _:S a :Person .
    () log:onNegativeSurface {
        [] :nested {
            _:S a :Human .
        } .
    } .
} .

# A query surface will contain the exact match of a RDF triple pattern
(_:S _:O) log:onQuerySurface {
    _:S :nested _:O .
} .

# An outputSurface in a query surface created a filter on the triples to
# show
(_:S _:O) log:onQuerySurface {
    _:S :nested _:O .
    () log:onOutputSurface _:O .
} .

# This will not work. An output surface can only be used in a nested
# query surface
() log:onOutputSurface {
    :Alice a :Cat .
} .

# Make the ./make_example.sh happy
:test :is true .
() log:onQuerySurface {
    :test :is true .
} .
