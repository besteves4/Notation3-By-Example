# Test 4

() log:negativeSurface {
    () log:negativeSurface {
        :Dave :eats :Quiche .
    } .
} .

() log:negativeSurface {
    () log:negativeSurface {
        () log:negativeSurface {
            :Estelle a :RealProgrammer.
        } .
    } .
} .

#
##############################################

# An exclusive disjunction P <-/-> Q means that P is true or Q but not both
# This is same as
#     P <-> ~Q
# and is the same as
#     ( P -> ~Q ) AND (~Q -> P) AND ( ~P -> Q ) AND ( Q -> ~ P )
# P = real programmer
# Q = eat quiche
(_:S) log:negativeSurface {

    _:S a :RealProgrammer .

    () log:negativeSurface {
    
        () log:negativeSurface {
            _:S :eats :Quiche  .
        } .
    } .
}.

(_:S) log:negativeSurface {

    () log:negativeSurface {
        _:S a :RealProgrammer .
    } .

    () log:negativeSurface {
        _:S :eats :Quiche .
    } .
}.

(_:S) log:negativeSurface {

    _:S :eats :Quiche .
 
    () log:negativeSurface {
        () log:negativeSurface {
            _:S a :RealProgrammer .
        } .
    } .
}.

# Q (eat quiche) -> P ( you are a programmer)
#
# Q -> P = ~ ( Q AND ~P )
(_:S) log:negativeSurface {
    _:S :eats :Quiche .

    () log:negativeSurface {
        _:S a :Programmer .
    } .
} .

(_:S _:C) log:querySurface {
    _:S a _:C.
} .

(_:S _:C) log:querySurface {
    _:S :eats _:C .
} .

(_:S _:C) log:querySurface {
    () log:negativeSurface { _:S :eats _:C } .
} .

(_:S _:C) log:querySurface {
    () log:negativeSurface { _:S a _:C } .
} .

# Test
() log:negativeSurface {
    :Alice a :RealProgrammer .
    :Bob a :Programmer .
    :Charly a :Programmer .
    :Dave a :Programmer .
    :Estelle a :Programmer .
    () log:negativeSurface {
        :test :is true .
    } .
} .

() log:querySurface {
    :test :is true .
} .