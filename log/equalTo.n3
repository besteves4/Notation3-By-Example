@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Mika a :Cat .

{
    # Evaluate if two RDF nodes are the same
    #    X a any 
    #    Y a any
    1 log:equalTo 1 .
    "Cat" log:equalTo "Cat" .

    # This will evaluate to false
    # "Cat" log:equalTo "Cat"@en .

    :Mika log:equalTo :Mika .

    # All exitstial are equal
    [ ] log:equalTo [ ] .
    _:x log:equalTo 1 .
    
    # Even Mika is something
    :Mika log:equalTo [ ] .

    # But :Mila is not equal to _:x because that is already equal to 1
    # Will fail: :Mika log:equalTo :_x .

    _:y log:equalTo _:x .
    _:y log:equalTo 1 .
    # But this will fail: _:y log:equalTo 2 (hint _:x already bound to 1 )
}
=> 
{   
    :Result :is true .
} .