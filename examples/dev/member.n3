# Start Implementation

{
    ?X dev:member ?Y 
}
<=
{
    ?X list:firstRest (?H ?T) .
    (?T ?Y) dev:member_ ?H .
} .

{
    (?X ?Y) dev:member_ ?Y.
}
<= true .

